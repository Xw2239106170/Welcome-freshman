
.turntable_wp {
  width: 610rpx;
  height: 610rpx;
  position: relative;
  margin: 0 auto;
}
.turntable_wp image {
  display: block;
  width: 100%;
  height: 100%;
}
.turntable_wp .turntable_pointer {
  position: absolute;
  width: 224rpx;
  height: 224rpx;
  top: 50%;
  left: 50%;
  margin: -125rpx 0 0 -112rpx;
}

