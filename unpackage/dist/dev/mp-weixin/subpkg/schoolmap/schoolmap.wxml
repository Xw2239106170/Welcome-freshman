<view class="content"><view class="tabs"><text data-event-opts="{{[['tap',[['showModelView',['$event']]]]]}}" class="tab" bindtap="__e">模型视图</text><text data-event-opts="{{[['tap',[['showSatelliteView',['$event']]]]]}}" class="tab" bindtap="__e">卫星视图</text></view><map hidden="{{!(showSatellite)}}" class="map" id="map" show-location="true" latitude="{{latitude}}" longitude="{{longitude}}" scale="16.8" enable-3D="true" skew="40" enable-traffic="true" enable-overlooking="true" enable-rotate="true" enable-satellite="true" markers="{{marker}}"></map><view hidden="{{!(showModel)}}" class="model-view"><map style="width:100%;height:100vh;" latitude="{{latitude}}" longitude="{{longitude}}" scale="16.8" enable-3D="true" skew="60" enable-traffic="true" enable-overlooking="true" show-location="true" enable-rotate="true" markers="{{marker}}"></map></view></view>