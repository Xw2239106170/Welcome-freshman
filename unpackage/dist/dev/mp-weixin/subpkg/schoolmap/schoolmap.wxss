
.content {
  flex: 1;
  width: 100%;
  height: 100vh;
  position: relative;
}
.tabs {
  display: flex;
  justify-content: space-around;
  margin-bottom: 10px;
}
.tab {
  padding: 10px;
  font-size: 16px;
  cursor: pointer;
}
.map,
.model-view {
  /* display: none; */
  width: 100%;
  height: 100vh;
  /* 40px 是 tab 的高度和下边距 */
}
.model-view {
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #f0f0f0;
}

